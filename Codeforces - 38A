#include <bits/stdc++.h>
using namespace std;

int main ()
{
    int n , x[100] , a , b , sum = 0;
    scanf("%d",&n);
    for (int i = 1; i < n; i++){
        scanf("%d",&x[i]);
    }
    scanf("%d%d",&a,&b);
    for (int i = a; i <= b-1; i++)
    {
        sum+= x[i];
    }
    printf("%d\n",sum);
    return 0;
}
