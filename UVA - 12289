#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n , i , length;
    string word;
    cin >> n;
    for (i = 0 ; i < n ; i++){
        cin >> word;
        if (word.size() == 5){
            cout <<"3" << endl;
        }
        else if (word.size() == 3 && word[0] == 't' && word[1] == 'w' || word[0] == 't' && word[2] == 'o' || word[1] == 'w' && word[2] == 'o'){
            cout <<"2" << endl;
        }
        else {
            cout <<"1" << endl;
        }
    }
    return 0;
}
